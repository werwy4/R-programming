data(airquality)
mean_temp <- sum(airquality$Temp, na.rm = TRUE) / sum(!is.na(airquality$Temp))
head(airquality, 5)
airquality_subset <- airquality %>% select(-Temp, -Wind)
coldest_day <- airquality[which.min(airquality$Temp), ]
summary(airquality)
melted_air <- melt(airquality)
melted_air_ID <- melt(airquality, id.vars = c("Month", "Day"))
casted_data <- dcast(melted_air_ID, Month + Day ~ variable, mean)
cast_avg_month <- dcast(melted_air_ID, Month ~ variable, mean)
